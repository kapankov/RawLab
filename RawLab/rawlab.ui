/* QScrollBar ------------------------------------------------------------- */
QScrollBar:horizontal {
background-color: #909090;
height: 16px;
margin: 1px 16px 1px 16px;
border: 1px solid #707070;
border-radius: 2px;
}
/* scrollBar slider */
QScrollBar::handle:horizontal {
background-color: #C0C0C0;
border: 1px solid #808080;
min-width: 8px;
border-radius: 2px;
}
QScrollBar::handle:horizontal:hover {
background-color: #E0E0E0;
border: 1px solid #808080;
min-width: 8px;
border-radius: 2px;
}
/* page-down */
QScrollBar::add-line:horizontal {
margin: 0px 2px 0px 2px;
border-image: url(:/images/Resources/right_arrow_disabled.png);
width: 10px;
height: 10px;
subcontrol-position: right;
subcontrol-origin: margin;
}
QScrollBar::add-line:horizontal:hover,
QScrollBar::add-line:horizontal:on {
border-image: url(:/images/Resources/right_arrow.png);
height: 10px;
width: 10px;
subcontrol-position: right;
subcontrol-origin: margin;
}
/* page-up */
QScrollBar::sub-line:horizontal {
margin: 0px 2px 0px 2px;
border-image: url(:/images/Resources/left_arrow_disabled.png);
height: 10px;
width: 10px;
subcontrol-position: left;
subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal:hover,
QScrollBar::sub-line:horizontal:on {
border-image: url(:/images/Resources/left_arrow.png);
height: 10px;
width: 10px;
subcontrol-position: left;
subcontrol-origin: margin;
}
/* left-right-arrow */
QScrollBar::up-arrow:horizontal,
QScrollBar::down-arrow:horizontal {
background: none;
}
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:horizontal {
background: none;
}

QScrollBar:vertical {
background-color: #909090;
width: 16px;
margin: 16px 1px 16px 1px;
border: 1px solid #707070;
border-radius: 2px;
}
/* scrollBar slider */
QScrollBar::handle:vertical {
background-color: #C0C0C0;
border: 1px solid #808080;
min-height: 8px;
border-radius: 2px;
}
QScrollBar::handle:vertical:hover {
background-color: #E0E0E0;
border: 1px solid #808080;
min-height: 8px;
border-radius: 2px;
}
/* page-down */
QScrollBar::add-line:vertical {
margin: 2px 0px 2px 0px;
border-image: url(:/images/Resources/down_arrow_disabled.png);
height: 10px;
width: 10px;
subcontrol-position: bottom;
subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:hover,
QScrollBar::add-line:vertical:on {
border-image: url(:/images/Resources/down_arrow.png);
height: 10px;
width: 10px;
subcontrol-position: bottom;
subcontrol-origin: margin;
}
/* page-up */
QScrollBar::sub-line:vertical {
margin: 2px 0px 2px 0px;
border-image: url(:/images/Resources/up_arrow_disabled.png);
height: 10px;
width: 10px;
subcontrol-position: top;
subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical:hover,
QScrollBar::sub-line:vertical:on {
border-image: url(:/images/Resources/up_arrow.png);
height: 10px;
width: 10px;
subcontrol-position: top;
subcontrol-origin: margin;
}
/* up-down-arrow */
QScrollBar::up-arrow:vertical,
QScrollBar::down-arrow:vertical {
background: none;
}
QScrollBar::add-page:vertical,
QScrollBar::sub-page:vertical {
background: none;
}

